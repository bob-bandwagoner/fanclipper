{
    "manifest_version": 3,
    "name": "fanclipper",
    "description": "ファンが楽しい瞬間をシェアするのを助けるChrome拡張です",
    "version": "0.1",
    "action": {
        "default_popup": "popup/popup.html"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
    },
    "background": {
        "service_worker": "service-worker.js"
    },
    "permissions": [
        "tabs"
    ]
}